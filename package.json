{
  "name": "ticket-evolution-seatmaps",
  "version": "0.0.1",
  "description": "interactive seatmaps",
  "repository": {
    "url": "git@github.com:ticketevolution/seatmaps-client.git",
    "type": "git"
  },
  "main": "build/tevomaps.js",
  "author": "BRAD LARSON",
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=8"
  },
  "scripts": {
    "build": "webpack --progress",
    "start": "./node_modules/.bin/webpack-dev-server --config examples/using-module/webpack.config.babel.js --context examples/using-module --open",
    "analyze": "webpack --profile --json --progress > stats.json",
    "lint": "eslint . --cache && stylelint **/*.scss --cache"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": ".*\\.test\\.js$"
  },
  "dependencies": {
    "preact": "8.4.2",
    "preact-compat": "3.18.4",
    "react-toggled": "1.2.7",
    "react-transition-group": "2.5.1",
    "svg-pan-zoom": "3.6.0",
    "unfetch": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-require": "^3.0.1",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-flow": "^1.0.1",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "normalize-scss": "^7.0.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  }
}